{
  "name": "backend",
  "version": "1.0.50",
  "type": "module",
  "scripts": {
    "test": "bun ../test/run.ts",
    "test:server": "bun ../test/testServer.ts",
    "dev": "bun run --watch src/server.ts",
    "build": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile server src/server.ts",
    "start": "./server",
    "lint": "bun eslint .",
    "lint:fix": "bun eslint . --fix"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.1",
    "@elysiajs/openapi": "^1.4.12",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.0",
    "elysia": "latest",
    "ioredis": "^5.8.2",
    "jose": "^6.1.3",
    "nodemailer": "^7.0.11",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "@types/node": "^25.0.2",
    "@types/nodemailer": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "bun-types": "latest",
    "eslint": "^9.39.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "install": "^0.13.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "module": "src/server.js"
}